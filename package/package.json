{
  "name": "react-quizlet-flashcard",
  "version": "4.0.22",
  "main": "dist/index.js",
  "description": "Simple quizlet like flashcard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ABSanthosh/react-quizlet-flashcard.git"
  },
  "author": "ABSanthosh <a.b.santhosh02@gmail.com>",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "../README.md"
  ],
  "keywords": [
    "quizlet",
    "flashcard",
    "react",
    "quizlet-flashcard",
    "react-quizlet-flashcard",
    "flash card",
    "flash-card",
    "flip card"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "prepack": "cp ../README.md ./",
    "lint": "eslint . --ext ts,tsx --ignore-pattern 'docs/'",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest",
    "dev": "nodemon --watch src --exec 'ladle serve'",
    "yalc:publish": "yalc publish",
    "rebuild": "npm run build && npm run yalc:publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@ladle/react": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "ladle": "^0.0.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-preserve-directives": "^1.1.3",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./dist/index.css": "./dist/index.css",
    "./README.md": "./README.md"
  }
}
